@{
    Layout = null;
}

<table style="width:100%">
    <tr>
        <th>#</th>
        <th>類別</th>
        <th>日期</th>
        <th>金額</th>
    </tr>
    @{
        foreach (var myClass in Model.Tables)
        {
            <tr>
                <td>@myClass.Sn</td>
                <td>@myClass.Class</td>
                <td>@myClass.Date</td>
                <td>@myClass.Money</td>
            </tr>
        }
    }
</table>
